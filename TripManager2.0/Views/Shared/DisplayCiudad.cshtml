@using BizData.Entities
@model IEnumerable<Ciudad>

<table border="0">

    <tr>
        <th>
            ID
        </th>
        <th>
            Nombre
        </th>
        <th>
            Pais
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            @Html.Partial("Ciudad", item)
        </tr>

    }

</table>
