@using BizData.Entities
@using BizLogic.Administration
@model IEnumerable<Ciudad>

<h1>Ciudades</h1>
<table border="0">
    <tr>

        <th>
            Nombre
        </th>
        <th>
            Pais
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            @Html.Partial("Ciudad", item)
            <td>
                <button type="submit" class="button" name="id" value=@item.CiudadID> Eliminar</button>
            </td>
        </tr>

    }
    <tr>
        <td>
            @Html.ActionLink("Agregar", "AddCiudad", "User", new CiudadCommand())
        </td>
        <td></td>
        <td></td>
    </tr>
</table>
