@using BizLogic.Administration
@model CiudadCommand
@{
    ViewData["Title"] = "Añadir Ciudad";
}

<form method="post">
    <CENTER>
        <h3>Añadir Ciudad</h3>
        <div class="col-md-2 col-md-offset-3">
            <div class="form-group">
                <label>Nombre</label>
                <input asp-for="Nombre" class="form-control" />
                <span asp-validation-for="Nombre" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label>Pais</label>
                <input asp-for="paisName" class="form-control" />
                <span asp-validation-for="paisName" class="text-danger"></span>
            </div>
            <input type="submit" />
        </div>
    </CENTER>
</form>
